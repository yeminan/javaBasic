
public class ScoreProc {

	public static void main(String[] args) {
		//2차원
		String[] name={"길정훈","김동협","김성민","김철수","신길호"};
		int[][] scores = {{90,80,100},{60,70,80},{95,85,75},{95,80,70},{80,70,90}};
		int[] tot= {0,0,0}; //과목별총점
		float[] avg= {0.0f, 0.0f, 0.0f};
		int total = 0;//전체 총점
		float average = 0.0f;//전체평균
		 
		
		for(int i=0;i<name.length;i++) {
			System.out.print(name[i]+"=\t");
			for(int j=0;j<3;j++) {
				System.out.print(scores[i][j]+"\t");
				tot[j]+=scores[i][j];
			}
			System.out.print("\n");
		}
		 			
	}

}
