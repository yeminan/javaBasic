package sec2;

public class NullEx1 {

	public static void main(String[] args) {
		//int a = null; 기본형은 null값을 가질 수 엇다.
		String b= null;
		int c = 20;
		int d =20;
		System.out.println("c == d : "+(c == d));
		System.out.println("c != d : "+(c != d));
		
		int[] e = {10,20,30};
		int[] f = {10,20,30};
		System.out.println("e == f :"+(e == f));
		System.out.println("e != f :"+(e != f));
	}

}
